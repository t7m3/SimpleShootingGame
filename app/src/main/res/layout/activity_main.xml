<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.njc_t1.simpleshootinggame.MainActivity">

    <ImageView
        android:id="@+id/imageViewEnemy"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rocket" />

    <ImageView
        android:id="@+id/imageViewPlayer"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/arw03up"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="366dp" />

    <ImageView
        android:id="@+id/imageViewExplosion"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/misc39b"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="430dp" />

    <ImageView
        android:id="@+id/imageViewBullet"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/arw02up"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="199dp" />

</android.support.constraint.ConstraintLayout>
